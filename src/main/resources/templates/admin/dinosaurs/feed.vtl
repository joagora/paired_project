<div class="blurry-image"></div>
<p class="pageHeader">Feed $dinosaur.getSpecies().getName()</p>
<form class="form-container small-form" action="/admin/dinosaurs/$dinosaur.getId()/feed" method="post">

    <label for="foodType" id="foodType">Select a Food</label>
    <select name ="foodType">
        #foreach ($foodType in $foodTypes)
        <option class="input-field" value="$foodType">$foodType.getName()</option>
        #end
    </select>

    <input type="submit" value="Feed">
</form>